  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "preLaunchTask": "typescript",
      "program": "${workspaceFolder}/src/main.ts",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    }, {
      "type": "node",
      "request": "launch",
      "name": "Debug tests/telegram",
      "preLaunchTask": "typescript",
      "program": "${workspaceFolder}/src/tests/telegramTest.ts",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    }, {
      "type": "node",
      "request": "launch",
      "name": "Debug tests/openPage",
      "preLaunchTask": "typescript",
      "program": "${workspaceFolder}/src/tests/pageTest.ts",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}